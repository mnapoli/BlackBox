{
    "name": "mnapoli/blackbox",
    "license": "MIT",
    "type": "library",
    "autoload": {
        "psr-4": {
            "BlackBox\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\BlackBox\\": "tests/"
        }
    },
    "require": {
        "php": ">=5.4",
        "symfony/finder": "~2.5"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "phpseclib/phpseclib": "*",
        "symfony/yaml": "~2.6",
        "jms/serializer": "~0.15",
        "predis/predis": "~1.0",
        "symfony/filesystem": "~2.5",
        "doctrine/dbal": "~2.4",
        "doctrine/cache": "~1.3",
        "mockery/mockery": "~0.9"
    },
    "suggest": {
        "doctrine/cache": "Required for CacheListStorage and CacheMapStorage (~1.3)"
    }
}
